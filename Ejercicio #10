class Rectangulo {

  double largo;
  double ancho;

  Rectangulo({required this.largo, required this.ancho});

  double calcularArea() {
    return largo * ancho;
  }
}

void main() {
  print('Este programa calcula el area de un rectangulo');
  Rectangulo miRectangulo = Rectangulo(largo: 5.0, ancho: 3.0);
  double area = miRectangulo.calcularArea();
  print('El área del rectángulo es: $area');
}
